{
  "manifest_version": 2,

  "name": "Hipsum",
  "description": "Grabs some Hipster Ipsum from context menus.",
  "version": "1.0",

  "permissions": [
    "http://hipsterjesus.com/",
    "https://hipsterjesus.com/",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}